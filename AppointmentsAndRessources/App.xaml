<Application x:Class="AppointmentAndRessources.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local1="clr-namespace:AppointmentsAndRessources">
    
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Ressources/UserControlWeekDay.xaml"/>
                <ResourceDictionary Source="Ressources/StylesDictionary.xaml"/>
                <ResourceDictionary Source="Ressources/DataTemplates.xaml"/>

                <ResourceDictionary>

                    <local1:Bootstrapper x:Key="Bootsrapper"/>
                </ResourceDictionary>
                
                          
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>



       

    </Application.Resources>
</Application>

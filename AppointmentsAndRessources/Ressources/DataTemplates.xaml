<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:AppointmentsAndRessources.Ressources">


    <DataTemplate x:Key="PatientenAuswahlComboBox">
        <StackPanel Orientation="Horizontal">
            <TextBlock Text="{Binding ANREDE}" MinWidth="50"/>
            <TextBlock Text="{Binding N_NAME}" MinWidth="120"/>
            <TextBlock Text="{Binding V_NAME}" MinWidth="120"/>
            <TextBlock Text="{Binding id}" MinWidth="120"/>
            

        </StackPanel>
    </DataTemplate>
    
</ResourceDictionary>